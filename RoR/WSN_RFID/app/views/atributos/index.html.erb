<table class="table table-striped">
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Valor</th>
      <th>Tipo variable</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @atributos.each do |atributo| %>
      <tr>
        <td><%= atributo.nombre_at %></td>
        <td><%= atributo.valor %></td>
        <td><%= atributo.tipo_variable %></td>
        <td><%= link_to 'Ver', atributo %></td>
        <td><%= link_to 'Editar', edit_atributo_path(atributo) %></td>
        <td><%= link_to 'Destroy', atributo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Atributo', new_atributo_path %>
