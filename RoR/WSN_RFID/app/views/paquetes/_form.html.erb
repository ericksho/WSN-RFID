<%= form_for(@paquete) do |f| %>
  <% if @paquete.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@paquete.errors.count, "error") %> prohibited this paquete from being saved:</h2>

      <ul>
      <% @paquete.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

<div class="container">
  <div class="col-md-6 column">
    <form role="form">
      <div class="form-group">
         <%= f.label :nombre %> <%= f.text_field :nombre, :class => "form-control" %>
      </div>
      <div class="form-group">
         <%= f.label "Valores por defecto" %><%= f.text_field :valores_dez, :class => "form-control" %>
      </div>
      <div class="form-group">
         <%= f.label :tipo %> <%= f.text_field :tipo, :class => "form-control" %>
      </div>
      <%= f.submit @label, :class => "btn btn-default" %>
  </div>
</div>
<% end %>
